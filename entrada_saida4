//===================================================================
//Faca um programa que leia 1 valor inteiro e mostre na tela
// a tabudada de multiplcacao da mesma.
//DICA:  Use \n pra quebrar linha   boa sorte ;)
//===================================================================

#include <stdio.h>

int main(){

int a;
    printf("Digite um numero: \n ");
    scanf("%d", &a);


int b = 0;
        int produto = a*b;
        printf("%d \n", produto);


int c = 1;
        int produto1 = a*c;
        printf("%d \n", produto1);


int d = 2;
    int produto2 = a*d;
    printf("%d \n", produto2);


int e = 3;
    int produto3 = a*e;
    printf("%d \n", produto3);


int f = 4;
    int produto4 = a*f;
    printf("%d \n", produto4);


int g = 5;
    int produto5 = a*g;
    printf("%d \n", produto5);


int h = 6;
    int produto6 = a*h;
    printf("%d \n", produto6);


int i = 7;
    int produto7 = a*i;
    printf("%d \n", produto7);


int j = 8;
    int produto8 = a*j;
    printf("%d \n", produto8);


int k =9;
    int produto9 = a*k;
    printf("%d \n", produto9);


int l= 10;
    int produto10 = a*l;
    printf("%d\n", produto10);

return 0;

}
